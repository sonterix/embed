{
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "script"
  },
  "env": {
    "browser": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "rules": {
    "max-len": [2, 120, 2, { "ignoreUrls": true }],
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off"
  }
}
